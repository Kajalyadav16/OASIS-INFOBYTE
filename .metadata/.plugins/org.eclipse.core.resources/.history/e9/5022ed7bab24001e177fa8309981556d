package array;
import java.util.*;

public class arry {
   public static void main(String args[])
   {
	 
	  
	   Scanner sc= new Scanner(System.in);
	   int a =sc.nextInt();
	   System.out.println("a");
	   int b =sc.nextInt();
	   System.out.println("b");
	   int c=a+b;
	   
	   System.out.println("Result="+c);
	   
   }
}
